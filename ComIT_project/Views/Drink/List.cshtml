@using DrinkAndGo.Data.Models
@model IEnumerable<Drink>

<div>
    <h2>All Drinks are shown here</h2>
</div>

<h2>@ViewBag.Name</h2>

@{ 
    foreach (var drink in Model)
    {
    <div>
        <h4>Drink Name: @drink.Name</h4>
        <p>Price: @drink.Price.ToString("c")</p>
    </div>
    }
}